{
  "name": "everluitonsk/citadel-client",
  "description": "Client library for interacting with the Citadel API",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Ivan Barlog",
      "email": "ivan.barlog@everlution.sk"
    }
  ],
  "require": {
    "php": "^8.0",
    "psr/http-client": "^1.0",
    "psr-discovery/http-factory-implementations": "*",
    "psr-discovery/http-client-implementations": "*"
  },
  "suggest": {
    "guzzlehttp/guzzle": "A PSR-18 compliant client package that also provides the necessary message factories"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/BeeSolve/citadel-php"
    }
  ],
  "autoload": {
    "psr-4": {
      "CitadelClient\\": "src/"
    }
  }
}
